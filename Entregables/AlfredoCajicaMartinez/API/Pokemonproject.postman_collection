						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"//Prueba para validar que status sea 200\r",
									"pm.test(\"Status code = 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"\r",
									"//Función de prueba que valide el tiempo de respuesta bajo de 700 ms\r",
									"pm.test(\"Tiempo de respuesta bajo de 700ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(700);\r",
									"});\r",
									"\r",
									"\r",
									"//funcion para validar el json schema de movimientos\",\r",
									"pm.test('Schema sea valido (jsonSchema)', function () {\r",
									"    pm.response.to.have.jsonSchema(mov_schema);\r",
									"});\r",
									"\r",
									"pm.environment.unset(\"movements_url\");"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{movements_url}}",
							"host": [
								"{{movements_url}}"
							]
						}
					},
					"response": []
				}
			]
		},
		{
			"name": "Negative Scenarios",
			"item": [
				{
					"name": "Get invalid pokemon info",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"//Prueba para validar que status sea 404 con pokemon invalido\r",
									"pm.test(\"Status code = 404\", function () {\r",
									"    pm.response.to.have.status(404);\r",
									"});\r",
									"\r",
									"\r",
									"//Prueba para validar que status sea Not found con pokemon invalido\r",
									"pm.test(\"Status code = Not Found\", function () {\r",
									"    pm.response.to.have.status(\"Not Found\");\r",
									"});\r",
									"\r",
									"\r",
									"//Función de prueba que valide el tiempo de respuesta arriba de 700 ms\r",
									"pm.test(\"Tiempo de respuesta arriba de 700ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.above(700);\r",
									"});\r",
									"\r",
									"\r",
									"pm.environment.unset(\"invalid_pokemon\");"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"pm.environment.set(\"invalid_pokemon\", \"no_existe\");"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{url}}pokemon/{{invalid_pokemon}}",
							"host": [
								"{{url}}pokemon"
							],
							"path": [
								"{{invalid_pokemon}}"
							]
						}
					},
					"response": []
				},
				{
					"name": "Get invalid pokemon first move",
					"event": [
